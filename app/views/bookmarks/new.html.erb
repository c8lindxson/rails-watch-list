<div class="container">
  <div class="navbar navbar-dark bg-secondary p-2 mt-2 align-items-end" style="height: 150px;">
    <h1>Add bookmark to list</h1>
    <%= link_to "go back", list_path(params[:list_id]), class:"btn btn-primary mt-1" %>
  </div>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit class:"btn btn-secondary" %>
  <% end %>
</div>
